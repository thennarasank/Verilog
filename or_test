#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5885db3b5200 .scope module, "tb_or_gate" "tb_or_gate" 2 8;
 .timescale 0 0;
v0x5885db3c7310_0 .var "a", 0 0;
v0x5885db3c73e0_0 .var "b", 0 0;
v0x5885db3c74b0_0 .net "y", 0 0, L_0x5885db3c75b0;  1 drivers
S_0x5885db3b5390 .scope module, "dut" "or_gate" 2 13, 2 2 0, S_0x5885db3b5200;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x5885db3c75b0 .functor OR 1, v0x5885db3c7310_0, v0x5885db3c73e0_0, C4<0>, C4<0>;
v0x5885db3b55c0_0 .net "a", 0 0, v0x5885db3c7310_0;  1 drivers
v0x5885db3c7130_0 .net "b", 0 0, v0x5885db3c73e0_0;  1 drivers
v0x5885db3c71f0_0 .net "y", 0 0, L_0x5885db3c75b0;  alias, 1 drivers
    .scope S_0x5885db3b5200;
T_0 ;
    %vpi_call 2 15 "$monitor", "$time=%ot | a=%b | b=%b | y=%b |", $time, v0x5885db3c7310_0, v0x5885db3c73e0_0, v0x5885db3c74b0_0 {0 0 0};
    %vpi_call 2 16 "$dumpfile", "or.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5885db3c7310_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5885db3c73e0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 7, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 24 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x5885db3c7310_0, 0, 1;
    %vpi_func 2 25 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x5885db3c73e0_0, 0, 1;
    %delay 10, 0;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "or_gate.v";
