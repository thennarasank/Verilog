#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c2b504aa200 .scope module, "xor_tb" "xor_tb" 2 10;
 .timescale 0 0;
v0x5c2b504bc1c0_0 .var "a", 0 0;
v0x5c2b504bc290_0 .var "b", 0 0;
v0x5c2b504bc360_0 .net "y", 0 0, L_0x5c2b504bc460;  1 drivers
S_0x5c2b504aa390 .scope module, "dut" "xor_gate" 2 15, 2 2 0, S_0x5c2b504aa200;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x5c2b504bc460 .functor XOR 1, v0x5c2b504bc1c0_0, v0x5c2b504bc290_0, C4<0>, C4<0>;
v0x5c2b504aa5c0_0 .net "a", 0 0, v0x5c2b504bc1c0_0;  1 drivers
v0x5c2b504bbfe0_0 .net "b", 0 0, v0x5c2b504bc290_0;  1 drivers
v0x5c2b504bc0a0_0 .net "y", 0 0, L_0x5c2b504bc460;  alias, 1 drivers
    .scope S_0x5c2b504aa200;
T_0 ;
    %vpi_call 2 18 "$monitor", "$time=%0t | a=%b | b=%b | y=%b|", $time, v0x5c2b504bc1c0_0, v0x5c2b504bc290_0, v0x5c2b504bc360_0 {0 0 0};
    %vpi_call 2 19 "$dumpfile", "xor.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c2b504bc1c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c2b504bc290_0, 0, 1;
    %delay 7, 0;
    %pushi/vec4 10, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 25 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x5c2b504bc1c0_0, 0, 1;
    %vpi_func 2 26 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x5c2b504bc290_0, 0, 1;
    %delay 8, 0;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "xor_gate.v";
